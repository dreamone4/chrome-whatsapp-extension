{
    "manifest_version": 2,
  
    "name": "Chrome Whatsapp Extension",
    "description": "This extension routes Whatsapp messages from web.whatsapp.com to a server",
    "version": "0.1.0",
  
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html",
      "default_title": "Click here!"
    },
    "permissions": [
      "activeTab",
      "storage"
    ]
  }
